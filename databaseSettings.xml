<application>
  <component name="DatabaseSettings" version="3">
    <option name="page-size" value="5000" />
    <text-mode in-scripts="false">
      <parameter-patterns>
        <parameter-pattern value="\?\d*" />
        <parameter-pattern value="\:\w+" />
        <parameter-pattern value="\@\w+" />
        <parameter-pattern value="\%\w+" />
        <parameter-pattern value="\$\{[^$\{\}]*\}" />
        <parameter-pattern value="\$(\w+)\$" scope="*,-PostgreSQL" />
        <parameter-pattern value="\#\w+\#" />
        <parameter-pattern value="\?(\d+)" in-scripts="false" in-literals="false" />
        <parameter-pattern value=":(\w+)" in-scripts="false" in-literals="false" scope="*,-PostgreSQL" />
        <parameter-pattern value="%\w+" in-scripts="false" in-literals="false" scope="JAVA,Python" predefined="%name" />
        <parameter-pattern value="\$\{([^\{\}]*)\}" in-scripts="false" in-literals="false" predefined="${name}" />
        <parameter-pattern value="\$\(([^\)]+)\)" in-scripts="false" in-literals="false" predefined="$(name)" />
        <parameter-pattern value="#(\w+)#" in-scripts="false" in-literals="false" scope="XML" predefined="#name#" />
        <parameter-pattern value="(?&lt;=\W|\A)\$(\d+|[a-zA-Z_](?:\w|\.)*)\$?(?=\W|\z)" in-scripts="false" in-literals="false" scope="*,-SQL" predefined="$a.b.c$?" />
        <parameter-pattern value="(?&lt;=\W|\A)#(\d+|[a-zA-Z_](?:\w|\.)*)#?(?=\W|\z)" in-scripts="false" in-literals="false" scope="*,-SQL" predefined="#a.b.c#?" />
        <parameter-pattern value="%\((\w+)\)s" in-scripts="false" in-literals="false" scope="Python" predefined="%(name)s" />
      </parameter-patterns>
    </text-mode>
    <csv-formats>
      <csv-format name="Tab Separated Values (TSV)" id="0">
        <data>
          <record-format prefix="" suffix="" nullText="" quotationPolicy="as needed" valueSeparator="&#9;" recordSeparator="&#10;">
            <quotation>
              <quotes left="&quot;" right="&quot;" leftEscaped="&quot;&quot;" rightEscaped="&quot;&quot;" />
              <quotes left="'" right="'" leftEscaped="''" rightEscaped="''" />
            </quotation>
          </record-format>
        </data>
        <header>
          <record-format prefix="" suffix="" nullText="" quotationPolicy="as needed" valueSeparator="&#9;" recordSeparator="&#10;">
            <quotation>
              <quotes left="&quot;" right="&quot;" leftEscaped="&quot;&quot;" rightEscaped="&quot;&quot;" />
              <quotes left="'" right="'" leftEscaped="''" rightEscaped="''" />
            </quotation>
          </record-format>
        </header>
      </csv-format>
      <csv-format name="Comma Separated Values (CSV)" id="1">
        <data>
          <record-format prefix="" suffix="" nullText="" quotationPolicy="as needed" valueSeparator="," recordSeparator="&#10;">
            <quotation>
              <quotes left="&quot;" right="&quot;" leftEscaped="&quot;&quot;" rightEscaped="&quot;&quot;" />
              <quotes left="'" right="'" leftEscaped="''" rightEscaped="''" />
            </quotation>
          </record-format>
        </data>
        <header>
          <record-format prefix="" suffix="" nullText="" quotationPolicy="as needed" valueSeparator="," recordSeparator="&#10;">
            <quotation>
              <quotes left="&quot;" right="&quot;" leftEscaped="&quot;&quot;" rightEscaped="&quot;&quot;" />
              <quotes left="'" right="'" leftEscaped="''" rightEscaped="''" />
            </quotation>
          </record-format>
        </header>
      </csv-format>
    </csv-formats>
  </component>
</application>