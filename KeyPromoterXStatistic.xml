<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Add New Row">
        <Item>
          <IdeaActionID>Console.TableResult.AddRow</IdeaActionID>
          <ShortCut>'⌃⏎'</ShortCut>
          <Description>Add New Row</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Data Source Properties">
        <Item>
          <IdeaActionID>DatabaseView.PropertiesAction</IdeaActionID>
          <ShortCut>'⌘I'</ShortCut>
          <Description>Data Source Properties</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Database">
        <Item>
          <IdeaActionID>ActivateDatabaseToolWindow</IdeaActionID>
          <ShortCut>'⌘2'</ShortCut>
          <Description>Database</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'⌘B' or '⌘Button1 Click' or 'Button2 Click'</ShortCut>
          <Description>Declaration</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Delete...">
        <Item>
          <IdeaActionID>$Delete</IdeaActionID>
          <ShortCut>'⌦' or '⌫' or '⌘⌫'</ShortCut>
          <Description>Delete...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Find">
        <Item>
          <IdeaActionID>ActivateFindToolWindow</IdeaActionID>
          <ShortCut>'⌘3'</ShortCut>
          <Description>Find</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Find Usages">
        <Item>
          <IdeaActionID>FindUsages</IdeaActionID>
          <ShortCut>'⌥F7'</ShortCut>
          <Description>Find Usages</Description>
          <Count>6</Count>
        </Item>
      </Statistic>
      <Statistic Action="Generate DDL to Clipboard">
        <Item>
          <IdeaActionID>DatabaseView.CopyDdlAction</IdeaActionID>
          <ShortCut>'⇧⌘C'</ShortCut>
          <Description>Generate DDL to Clipboard</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Go to next highlighted element usage">
        <Item>
          <IdeaActionID>GotoNextElementUnderCaretUsage</IdeaActionID>
          <ShortCut>'⌃⌥↓'</ShortCut>
          <Description>Go to next highlighted element usage</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Hide Active Tool Window">
        <Item>
          <IdeaActionID>HideActiveWindow</IdeaActionID>
          <ShortCut>'⇧⎋' or '⌘W'</ShortCut>
          <Description>Hide Active Tool Window</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Jump to Console...">
        <Item>
          <IdeaActionID>Jdbc.OpenConsole.Any</IdeaActionID>
          <ShortCut>'⇧⌘F10'</ShortCut>
          <Description>Jump to Console...</Description>
          <Count>7</Count>
        </Item>
      </Statistic>
      <Statistic Action="Modify Table...">
        <Item>
          <IdeaActionID>ModifyObject</IdeaActionID>
          <ShortCut>'⌘F6'</ShortCut>
          <Description>Modify Table...</Description>
          <Count>8</Count>
        </Item>
      </Statistic>
      <Statistic Action="Move Right">
        <Item>
          <IdeaActionID>MoveTabRight</IdeaActionID>
          <ShortCut>'⌥⌘U'</ShortCut>
          <Description>Move Right</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Parameter Info">
        <Item>
          <IdeaActionID>ParameterInfo</IdeaActionID>
          <ShortCut>'⌘P'</ShortCut>
          <Description>Parameter Info</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Project">
        <Item>
          <IdeaActionID>ActivateProjectToolWindow</IdeaActionID>
          <ShortCut>'⌘1'</ShortCut>
          <Description>Project</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Reload Page">
        <Item>
          <IdeaActionID>Console.TableResult.Reload</IdeaActionID>
          <ShortCut>'⌘R'</ShortCut>
          <Description>Reload Page</Description>
          <Count>18</Count>
        </Item>
      </Statistic>
      <Statistic Action="Safe Delete...">
        <Item>
          <IdeaActionID>SafeDelete</IdeaActionID>
          <ShortCut>'⌘⌦'</ShortCut>
          <Description>Safe Delete...</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Show Diff">
        <Item>
          <IdeaActionID>Diff.ShowDiff</IdeaActionID>
          <ShortCut>'⌘D'</ShortCut>
          <Description>Show Diff</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Submit">
        <Item>
          <IdeaActionID>Console.TableResult.Submit</IdeaActionID>
          <ShortCut>'⌘⏎'</ShortCut>
          <Description>Submit</Description>
          <Count>6</Count>
        </Item>
      </Statistic>
      <Statistic Action="Synchronize">
        <Item>
          <IdeaActionID>DatabaseView.SynchronizeAction</IdeaActionID>
          <ShortCut>'⌥⌘Y'</ShortCut>
          <Description>Synchronize</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
      <Statistic Action="TODO">
        <Item>
          <IdeaActionID>ActivateTODOToolWindow</IdeaActionID>
          <ShortCut>'⌘6'</ShortCut>
          <Description>TODO</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'⌘F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>7</Count>
        </Item>
      </Statistic>
    </option>
    <option name="suppressed">
      <Statistic Action="Find in Path...">
        <Item>
          <IdeaActionID>FindInPath</IdeaActionID>
          <ShortCut>'⇧⌘F'</ShortCut>
          <Description>Find in Path...</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>