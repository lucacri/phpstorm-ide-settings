<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Data Source Properties">
        <Item>
          <IdeaActionID>DatabaseView.PropertiesAction</IdeaActionID>
          <ShortCut>'⌘I'</ShortCut>
          <Description>Data Source Properties</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Find Usages">
        <Item>
          <IdeaActionID>FindUsages</IdeaActionID>
          <ShortCut>'⌥F7'</ShortCut>
          <Description>Find Usages</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Find in Path...">
        <Item>
          <IdeaActionID>FindInPath</IdeaActionID>
          <ShortCut>'⇧⌘F'</ShortCut>
          <Description>Find in Path...</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Move Right">
        <Item>
          <IdeaActionID>MoveTabRight</IdeaActionID>
          <ShortCut>'⌥⌘U'</ShortCut>
          <Description>Move Right</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Parameter Info">
        <Item>
          <IdeaActionID>ParameterInfo</IdeaActionID>
          <ShortCut>'⌘P'</ShortCut>
          <Description>Parameter Info</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Project">
        <Item>
          <IdeaActionID>ActivateProjectToolWindow</IdeaActionID>
