          <Description>Safe Delete...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Submit">
        <Item>
          <IdeaActionID>Console.TableResult.Submit</IdeaActionID>
          <ShortCut>'⌘⏎'</ShortCut>
          <Description>Submit</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Synchronize">
        <Item>
          <IdeaActionID>DatabaseView.SynchronizeAction</IdeaActionID>
          <ShortCut>'⌥⌘Y'</ShortCut>
          <Description>Synchronize</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'⌘F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>4</Count>
        </Item>
